def MaxWeightInGraph(numbers):
    max_sum = 0
    tracker = [0 for i in range(len(numbers))]
    tracker[0] = 0
    tracker[1] = numbers[1]
    for i in range(2,len(numbers)):
        first_sum = tracker[i-1]
        second_sum = tracker[i-2]+numbers[i]
        if first_sum > second_sum:
            tracker[i] = first_sum
        else:
            tracker[i] = second_sum       
    return max(tracker)
